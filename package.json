{
  "name": "assets-optimizer",
  "version": "0.0.1",
  "description": "Bunch of assets optimizers (svg, png, jpg, etc...)",
  "repository": "https://github.com/StartupCraft/assets-optimizer.git",
  "author": "George Viazemsky <code@and.graphics>",
  "license": "GPL-3.0",
  "main": "./lib/index.js",
  "bin": {
    "optimize": "./lib/cli.js"
  },
  "scripts": {
    "build": "babel src/ -d lib/",
    "lint": "eslint src ",
    "lint:fix": "eslint src --fix",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,css}": [
      "eslint src --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-eslint": "9.0.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2"
  },
  "dependencies": {
    "filesize": "3.6.1",
    "glob": "7.1.3",
    "shelljs": "0.8.2",
    "svgo": "1.0.5"
  }
}
